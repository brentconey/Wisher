@model IEnumerable<Yearnly.Model.UserProfile>
@{
    Layout = null;
}
@{
    if(Model.Count() > 0)
    {
        foreach (var row in Model)
        {
            <li class="group friend">
                <span class="friend-name">@row.FirstName @row.LastName</span>
                <span class="friend-username">@row.UserName</span>
                <a class="add-friend button right" href="#">send friend request</a>
            </li>
        }
    }else{
        <li class="group friend">Nobody with that name round here.</li>   
    }
}
